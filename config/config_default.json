    "exclude": false
  },
  "ChaosFile": {
    "regex": ".*.(log|logs|xlog|cache|caches|tmp|temp|tmf|tlog|sys|hprof|xcrash)",
    "exclude": false
  },
  "ApkClean": {
    "regex": ".*.*(apk|apk.1|apkm|apks)$",
    "exclude": false
  }
}
